{
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/mongodb-odm-bundle": "^5.5",
    "nelmio/cors-bundle": "^2.2",
    "symfony/asset": "7.3.*",
    "symfony/cache": "7.3.*",
    "symfony/clock": "7.3.*",
    "symfony/console": "7.3.*",
    "symfony/dependency-injection": "7.3.*",
    "symfony/dotenv": "7.3.*",
    "symfony/error-handler": "7.3.*",
    "symfony/expression-language": "7.3.*",
    "symfony/flex": "^2.2",
    "symfony/framework-bundle": "7.3.*",
    "symfony/http-client": "7.3.*",
    "symfony/http-foundation": "7.3.*",
    "symfony/http-kernel": "7.3.*",
    "symfony/monolog-bundle": "^3.8",
    "symfony/property-access": "7.3.*",
    "symfony/property-info": "7.3.*",
    "symfony/routing": "7.3.*",
    "symfony/runtime": "7.3.*",
    "symfony/security-bundle": "7.3.*",
    "symfony/security-core": "7.3.*",
    "symfony/security-http": "7.3.*",
    "symfony/serializer": "7.3.*",
    "symfony/twig-bundle": "7.3.*",
    "symfony/twig-bridge": "7.3.*",
    "symfony/ux-react": "^2.10",
    "symfony/validator": "7.3.*",
    "symfony/var-exporter": "7.3.*",
    "symfony/webpack-encore-bundle": "^2.0",
    "symfony/yaml": "7.3.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "ext-mongodb": "*",
    "dg/bypass-finals": "^1.5",
    "mockery/mockery": "^1.6",
    "overtrue/phplint": "^9",
    "phpmd/phpmd": "^2.15.0",
    "phpunit/phpunit": "^10",
    "squizlabs/php_codesniffer": "*",
    "symfony/browser-kit": "7.3.*",
    "symfony/css-selector": "7.3.*",
    "symfony/debug-bundle": "7.3.*",
    "symfony/dom-crawler": "7.3.*",
    "symfony/maker-bundle": "^1.44",
    "symfony/phpunit-bridge": "7.3.*",
    "symfony/stopwatch": "7.3.*",
    "symfony/var-dumper": "7.3.*",
    "symfony/web-profiler-bundle": "7.3.*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "platform-check": false
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-intl-grapheme": "*",
    "symfony/polyfill-intl-normalizer": "*",
    "symfony/polyfill-mbstring": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "run-tests": "composer run-phpcs && composer run-phpunit && composer run-phpmd && composer run-phplint",
    "run-phpcs": "vendor/bin/phpcs",
    "run-phpmd": "vendor/bin/phpmd src text phpmd-src.xml --exclude src/Migrations/ && vendor/bin/phpmd tests text phpmd-tests.xml",
    "run-phplint": "vendor/bin/phplint",
    "run-phpunit": "vendor/bin/phpunit",
    "run-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-filter src/ --coverage-html coverage"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*",
      "docker": false
    }
  }
}
